  },
  "devDependencies": {
    "@types/jest": "25",
    "@types/node": "13",
    "@types/node-fetch": "2",
    "@typescript-eslint/eslint-plugin": "2",
    "@typescript-eslint/parser": "2",
    "eslint": "6",
    "eslint-config-prettier": "6",
    "eslint-plugin-prettier": "3",
    "jest": "25",
    "prettier": "2",
    "ts-jest": "25",
    "typescript": "3"
  },
  "dependencies": {
    "node-fetch": "2.6.1"
  }
}